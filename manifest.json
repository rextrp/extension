{
  "manifest_version": 2,

  "name": "VocabJET",
  "description": "Selected sentence will be sent to VocabJET builder for processing.",
  "version": "0.1",

  "browser_action": {
    "default_icon": 	"icon.png",
    "default_popup": 	"popup.html",
    "default_title": 	"VocabJET"
  },
  "icons": {
          "16": "icon16.png"
  },
  "permissions": [ "contextMenus" ],
  "background": { "persistent": false, "scripts": [ "popup.js" ] }  
}